{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "types": [
      "node",
      "vite/client",
      "@nuxt/types",
      "@nuxtjs/supabase"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@UI/*": [
        "./*"
      ],
      "~/*": [
        "./*"
      ],
    },
  },
}
