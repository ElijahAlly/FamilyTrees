{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "node",
      "vite/client",
      "@nuxt/types",
      "@nuxtjs/supabase",
      "@types/formidable"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@UI/*": [
        "./*"
      ],
      "~/*": [
        "./*"
      ],
    },
  },
}